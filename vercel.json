{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "app.py",
      "methods": ["POST"]
    },
    {
      "src": "/api/health",
      "dest": "app.py",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "app.py",
      "methods": ["GET"]
    }
  ]
}
